<templateSet group="Drupal 7">
  <template name="d7-form-textfield" value="$form['$NAME$'] = array(&#10;    '#type' =&gt; 'textfield',&#10;    '#title' =&gt; '$TITLE$',&#10;    '#description' =&gt; '$DESCRIPTION$',&#10;);" description="Textfield Form Item" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="d7-form-select" value="$form['$NAME$'] = array(&#10;    '#type' =&gt; 'select',&#10;    '#title' =&gt; '$TITLE$',&#10;    '#description' =&gt; '$DESCRIPTION$',&#10;    '#options' =&gt; array(),&#10;);" description="Select Form Item" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="d7-form-submit" value="$form['$NAME$'] = array(&#10;    '#type' =&gt; 'submit',&#10;    '#value' =&gt; '$TITLE$',&#10;);" description="Submit Form Item" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="d7-menu-item" value="    $items['$LINK$'] = array(&#10;      'title'            =&gt; '$TITLE$',&#10;      'page callback'    =&gt; '$CALLBACK$',&#10;      'page arguments'   =&gt; array(),&#10;      'access arguments' =&gt; array('access content'),&#10;      'file'             =&gt; '$FILE$.inc',&#10;      'file path'        =&gt; $path_to_module . '/includes',&#10;      'type'             =&gt; MENU_CALLBACK,&#10;    );" description="Drupal Menu Item" toReformat="false" toShortenFQNames="true">
    <variable name="LINK" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CALLBACK" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FILE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="d7-form-date-popup" value="    $format            = 'd.m.y';&#10;    $date              = time();&#10;    $date_f            = date(&quot;Y-m-d H:i:s&quot;, $date);&#10;    $form['$VAR1$']      = array(&#10;      '#type'                =&gt; 'date_popup',&#10;      '#title'               =&gt; '$VAR2$',&#10;      '#default_value'       =&gt; $date_f,&#10;      '#date_format'         =&gt; $format,&#10;      '#size'                =&gt; 10,&#10;      '#date_label_position' =&gt; 'within',&#10;      '#date_year_range'     =&gt; '-1:+1',&#10;      '#description'         =&gt; '',&#10;    );" description="Date Popup Form Item" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="d7-form-checkboxes" value="    $form['$var1$'] = array(&#10;      '#title'   =&gt; &quot;$var2$&quot;,&#10;      '#type'    =&gt; 'checkboxes',&#10;      '#options' =&gt; array(),&#10;    );" description="Checkboxes Form Item" toReformat="false" toShortenFQNames="true">
    <variable name="var1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="var2" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="d7-fieldset" value="$form['$VAR1$']                      = array(&#10;    '#type'        =&gt; 'fieldset',&#10;    '#title'       =&gt; '$VAR2$',&#10;    '#collapsible' =&gt; $VAR3$,&#10;    '#collapsed'   =&gt; $VAR4$,&#10;);" description="Form Fieldset" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR2" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR3" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR4" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>